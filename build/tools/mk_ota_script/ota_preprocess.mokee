FILES=("system/xbin/zip", "system/system/xbin/zip")

for i in "${FILES[@]}"; do
	parent=$(dirname $i)
	if [ ! -z $(check_f z2/$i) ];then
		mkdirp out/$parent 2> /dev/null
		mv z2/$i out/$parent/
	fi
	if [ ! -z $(check_f z1/$i) ];then
		rm z1/$i
	fi
done
